 TVV1 = THETA(2)
 TVQ  = THETA(3)
 TVV2 = THETA(4)
 TVKM = THETA(5)
 TVVM = THETA(6)

 MU_1 = LOG(TVCL)
 MU_2 = LOG(TVV1)
 MU_3 = LOG(TVQ)
 MU_4 = LOG(TVV2)
 MU_5 = LOG(TVKM)
 MU_6 = LOG(TVVM)

 CL = EXP(MU_1 + ETA(1))
 V1 = EXP(MU_2 + ETA(2))
 Q  = EXP(MU_3 + ETA(3))
 V2 = EXP(MU_4 + ETA(4))
 KM = EXP(MU_5 + ETA(5))
 VM = EXP(MU_6 + ETA(6))

 S1=V1

 K10 = CL/V1
 K12 = Q/V1
 K21 = Q/V2

     MU_7 = THETA(7)
     COV7 = MU_7 + ETA(7)
     MU_8 = THETA(8)
     COV8 = MU_8 + ETA(8)
     MU_9 = THETA(9)
     COV9 = MU_9 + ETA(9)
     MU_10 = THETA(10)
     COV10 = MU_10 + ETA(10)
     MU_11 = THETA(11)
     COV11 = MU_11 + ETA(11)
     MU_12 = THETA(12)
     COV12 = MU_12 + ETA(12)
     MU_13 = THETA(13)
     COV13 = MU_13 + ETA(13)
     MU_14 = THETA(14)
     COV14 = MU_14 + ETA(14)
     MU_15 = THETA(15)
     COV15 = MU_15 + ETA(15)
     MU_16 = THETA(16)
     COV16 = MU_16 + ETA(16)
     MU_17 = THETA(17)
     COV17 = MU_17 + ETA(17)
$DES         
 CP = A(1)/V1
 ELIM = CP*VM/(KM+CP)

 DADT(1) = K21*A(2) - K12*A(1) - K10*A(1) -ELIM
 DADT(2) = K12*A(1) - K21*A(2)

$ERROR        
 ;(OBSERVATION ONLY)

 IPRED = LOG(1)
 IF(F.GT.0) IPRED = LOG(F)

 W     = 1
 IRES  = DV-IPRED
 IWRES = IRES/W

 RES  = EPS(1)

 Y = IPRED+W*RES


;;;FREM CODE BEGIN COMPACT
;;;DO NOT MODIFY
     IF (FREMTYPE.EQ.100) THEN
;       LBWT  1
        Y = COV7 + EPS(2)
        IPRED = COV7
     END IF
     IF (FREMTYPE.EQ.200) THEN
;       LBLBW  1
        Y = COV8 + EPS(2)
        IPRED = COV8
     END IF
     IF (FREMTYPE.EQ.300) THEN
;       LBBMI  1
        Y = COV9 + EPS(2)
        IPRED = COV9
     END IF
     IF (FREMTYPE.EQ.400) THEN
;       LBBSA  1
        Y = COV10 + EPS(2)
        IPRED = COV10
     END IF
     IF (FREMTYPE.EQ.500) THEN
;       LAGE  1
        Y = COV11 + EPS(2)
        IPRED = COV11
     END IF
     IF (FREMTYPE.EQ.600) THEN
;       LBCCL  1
        Y = COV12 + EPS(2)
        IPRED = COV12
     END IF
     IF (FREMTYPE.EQ.700) THEN
;       DOSE_10  1
        Y = COV13 + EPS(2)
        IPRED = COV13
     END IF
     IF (FREMTYPE.EQ.800) THEN
;       DOSE_5  1
        Y = COV14 + EPS(2)
        IPRED = COV14
     END IF
     IF (FREMTYPE.EQ.900) THEN
;       SEX  1
        Y = COV15 + EPS(2)
        IPRED = COV15
     END IF
     IF (FREMTYPE.EQ.1000) THEN
;       RACE  1
        Y = COV16 + EPS(2)
        IPRED = COV16
     END IF
     IF (FREMTYPE.EQ.1100) THEN
;       STUT  1
        Y = COV17 + EPS(2)
        IPRED = COV17
     END IF
